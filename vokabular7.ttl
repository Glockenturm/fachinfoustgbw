@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/resource/> .

# Konzeptschema Definition
ex:scheme a skos:ConceptScheme ;
    dct:title "Klassifikation FachInfoUstgBw, 3. Auflage 2020."@de, 
              "Classification FachInfoUstgBw, 3rd Edition 2020."@en ;
    dct:description "Diese Klassifikation bietet Unterstützung der Fachinformation für die Bundeswehr."@de,
                    "This classification supports specialist information for the German Armed Forces."@en ;
    dct:issued "2020-01-01"^^<http://www.w3.org/2001/XMLSchema#date> ;
    skos:hasTopConcept ex:MIA .

# Hauptkonzept
ex:MIA a skos:Concept ;
    dct:audience "Angehörige der Bundeswehr"@de,
                 "Members of the Bundeswehr"@en ;
    dct:description "Erfasst Organisation, Strategie, Taktik, Geschichte und allgemeine Aspekte des Militärwesens."@de,
                    "Covers organization, strategy, tactics, history, and general aspects of military affairs."@en ;
    dct:language "de", "en" ;
    dct:title "Militärwesen allgemein"@de,
              "Military Affairs in General"@en ;
    skos:notation "MIA" ;
    skos:prefLabel "Militärwesen allgemein"@de,
                   "Military Affairs in General"@en ;
    skos:inScheme ex:scheme ;
    skos:narrower ex:MIA000, ex:MIA100 .

# Beispiel eines untergeordneten Konzepts
ex:MIA000 a skos:Concept ;
    dct:audience "Angehörige der Bundeswehr"@de,
                 "Members of the Bundeswehr"@en ;
    dct:description "Allgemeine Themen des Militärwesens werden erfasst."@de,
                    "Captures general issues of military affairs."@en ;
    dct:language "de", "en" ;
    dct:title "Militärwesen"@de,
              "Military Affairs"@en ;
    skos:notation "MIA000" ;
    skos:prefLabel "Militärwesen"@de,
                   "Military Affairs"@en ;
    skos:inScheme ex:scheme ;
    skos:narrower ex:MIA001 .

ex:MIA001 a skos:Concept ;
    dct:audience "Angehörige der Bundeswehr"@de,
                 "Members of the Bundeswehr"@en ;
    dct:description "Beinhaltet Fachenzyklopädien, Lexika und Fachwörterbücher zum Thema."@de,
                    "Includes encyclopedias, lexicons, and specialized dictionaries on the subject."@en ;
    dct:language "de", "en" ;
    dct:title "Allgemeine militärische Fachenzyklopädien, Lexika und Fachwörterbücher"@de,
              "General Military Encyclopedias, Lexicons, and Specialized Dictionaries"@en ;
    skos:notation "MIA001" ;
    skos:prefLabel "Allgemeine militärische Fachenzyklopädien, Lexika und Fachwörterbücher"@de,
                   "General Military Encyclopedias, Lexicons, and Specialized Dictionaries"@en ;
    skos:scopeNote """ausgenommen werden sollten Fachenzyklopädien der Land-, Luft- und Seestreitkräfte."""@de,
                   """Excludes specialized encyclopedias of Land, Air, and Naval Forces."""@en ;
    skos:inScheme ex:scheme .

# Ein weiteres Konzept
ex:MIA100 a skos:Concept ;
    dct:audience "Angehörige der Bundeswehr"@de,
                 "Members of the Bundeswehr"@en ;
    dct:description "Umfasst alle militärischen Strukturen eines Landes und auch nichtstaatliche militärische Gruppen."@de,
                    "Includes all military structures of a country as well as non-state military groups."@en ;
    dct:language "de", "en" ;
    dct:title "Wehrsysteme, Gesamtstreitkräfte, strategische Truppengliederungen, paramilitärische Organisationen"@de,
              "Military systems, overall armed forces, strategic troop structures, paramilitary organizations"@en ;
    skos:notation "MIA100" ;
    skos:prefLabel "Wehrsysteme, Gesamtstreitkräfte, strategische Truppengliederungen, paramilitärische Organisationen"@de,
                   "Defense Systems"@en ;
    skos:inScheme ex:scheme ;
    skos:narrower ex:MIA103, ex:MIA130 .

ex:MIA103 a skos:Concept ;
    dct:audience "Angehörige der Bundeswehr"@de,
                 "Members of the Bundeswehr"@en ;
    dct:description "Erforscht die Entwicklung und Veränderung militärischer Strukturen in der nationalen und internationalen Sicherheit."@de,
                    "Explores development and changes in military structures in national and international security."@en ;
    dct:language "de", "en" ;
    dct:title "Geschichte der Wehrsysteme, Gesamtstreitkräfte, strategischen Truppengliederungen und paramilitärischen Organisationen"@de,
              "History of military systems, overall armed forces, strategic troop structures, and paramilitary organizations"@en ;
    skos:notation "MIA103" ;
    skos:prefLabel "Geschichte der Wehrsysteme, Gesamtstreitkräfte, strategischen Truppengliederungen und paramilitärischen Organisationen"@de,
                   "History of Defense Systems, Unified Armed Forces, Strategic Force Structures, and Paramilitary Organizations"@en ;
    skos:scopeNote "Hinw: IMMER auch eine entsprechende MIG... Notation vergeben."@de,
                   "Note: ALWAYS assign a corresponding MIG... notation."@en ;
    skos:inScheme ex:scheme .
